<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="36dp"
    android:height="36dp"
    android:tint="?attr/colorControlNormal"
    android:viewportHeight="36"
    android:viewportWidth="36">
    <group
        android:translateX="16"
        android:translateY="16">
        <group android:name="ring_outer">
            <path
                android:name="ring_outer_path"
                android:pathData="@string/checkable_radiobutton_ring_outer_path"
                android:strokeColor="@android:color/white"
                android:strokeWidth="2" />
        </group>
        <group android:name="dot_group">
            <path
                android:name="dot_path"
                android:fillColor="@android:color/white"
                android:pathData="@string/checkable_radiobutton_dot_path" />
        </group>
    </group>
</vector>
